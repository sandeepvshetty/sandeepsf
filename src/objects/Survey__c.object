    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Holds a list of questions that comprise this survey</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableReports>false</enableReports>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <fields>
        <fullName>Completed_URL__c</fullName>
        <description>When the user clicks done from the last page, this URL will take them back somewhere.</description>
        <externalId>false</externalId>
        <label>Completed URL</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Description__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Description to display to the contact that completes the survey</inlineHelpText>
        <label>Description</label>
        <length>4000</length>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>5</visibleLines>
    </fields>
    <fields>
        <fullName>Questions_per_page__c</fullName>
        <defaultValue>10</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>How many questions will be presented on each page of the survey</inlineHelpText>
        <label>Questions per page</label>
        <precision>2</precision>
        <required>true</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Site_URL__c</fullName>
        <description>required since apex code cannot read the $Site global variable</description>
        <externalId>false</externalId>
        <inlineHelpText>The base URL of the site where the user will take the survey. Found in Sites setup. Copy the default web address including any subdirectories into this field.</inlineHelpText>
        <label>Site URL</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <label>Survey</label>
    <nameField>
        <label>Survey Name</label>
        <type>Text</type>
    </nameField>
    <pluralLabel>Surveys</pluralLabel>
    <searchLayouts>
        <customTabListAdditionalFields>Description__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Completed_URL__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Questions_per_page__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>CREATED_DATE</customTabListAdditionalFields>
        <searchFilterFields>NAME</searchFilterFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <webLinks>
        <fullName>Close_Survey</fullName>
        <availability>online</availability>
        <description>Remove the incomplete invitations from this survey</description>
        <displayType>link</displayType>
        <height>600</height>
        <linkType>page</linkType>
        <masterLabel>Close Survey</masterLabel>
        <openType>sidebar</openType>
        <page>removeUnusedInvites</page>
        <protected>false</protected>
    </webLinks>
    <webLinks>
        <fullName>Edit_Questions</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <height>600</height>
        <linkType>page</linkType>
        <masterLabel>Edit Questions</masterLabel>
        <openType>sidebar</openType>
        <page>surveyQuestionEdit</page>
        <protected>false</protected>
    </webLinks>
    <webLinks>
        <fullName>Invite_Contacts</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>700</height>
        <linkType>url</linkType>
        <masterLabel>Create Invitations</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <url>/apex/inviteContacts?vid={!Survey__c.Id}</url>
    </webLinks>
    <webLinks>
        <fullName>Send_Invitations</fullName>
        <availability>online</availability>
        <description>send up to 200 invitations</description>
        <displayType>link</displayType>
        <encodingKey>UTF-8</encodingKey>
        <height>600</height>
        <linkType>url</linkType>
        <masterLabel>Send Invitations</masterLabel>
        <openType>sidebar</openType>
        <protected>false</protected>
        <url>/apex/sendInvitations?vid={!Survey__c.Id}</url>
    </webLinks>
</CustomObject>
