    <enableStreamingApi>true</enableStreamingApi>
    <fields>
        <fullName>Completed__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Checked when the contact has used this invitation</description>
        <externalId>false</externalId>
        <label>Completed</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Contact__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Contact</label>
        <referenceTo>Contact</referenceTo>
        <relationshipLabel>Invitations</relationshipLabel>
        <relationshipName>Invitations</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Invite_Code__c</fullName>
        <externalId>false</externalId>
        <label>InviteCode</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Invite_Code_text__c</fullName>
        <externalId>false</externalId>
        <formula>Invite_Code__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Invite Code</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Invite_Sent__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>set by the system when an email is sent using this invitation</inlineHelpText>
        <label>Invite Sent</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Site_URL__c</fullName>
        <externalId>false</externalId>
        <formula>Survey__r.Site_URL__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Site URL</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Survey_Link__c</fullName>
        <description>build the survey link to be used in the outbound email</description>
        <externalId>false</externalId>
        <formula>if (  RIGHT(Site_URL__c, 1) == &apos;/&apos; ,
HYPERLINK( Site_URL__c &amp; &apos;surveyWizard?inv=&apos;&amp; Invite_Code__c , &apos;Launch Survey&apos; , &apos;_blank&apos; ) ,

HYPERLINK( Site_URL__c &amp; &apos;/surveyWizard?inv=&apos;&amp; Invite_Code__c , &apos;Launch Survey&apos; , &apos;_blank&apos; )

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Survey Link</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Survey__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Survey</label>
        <referenceTo>Survey__c</referenceTo>
        <relationshipLabel>Invitations</relationshipLabel>
        <relationshipName>Invitations</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>subject__c</fullName>
        <externalId>false</externalId>
        <formula>Survey__r.Name</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Subject</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Invitation</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Contact__c</columns>
        <columns>Invite_Code__c</columns>
        <columns>Survey__c</columns>
        <columns>Survey_Link__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <listViews>
        <fullName>Created_Today</fullName>
        <columns>NAME</columns>
        <columns>Contact__c</columns>
        <columns>Invite_Code_text__c</columns>
        <columns>Survey__c</columns>
        <columns>Survey_Link__c</columns>
        <filterScope>Everything</filterScope>
